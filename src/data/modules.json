            "itinerary": {
                "pdfUrl": "/pdfs/itinerary-noa.pdf",
                "days": [
                    {
                        "day": 1,
                        "title": "Llegada a Tucumán y Bienvenida",
                        "description": "Recepción en el aeropuerto, traslado al hotel y cena de bienvenida para presentar el programa."
                    },
                    {
                        "day": 2,
                        "title": "City Tour Histórico y San Javier",
                        "description": "Visita a la Casa Histórica y Plaza Independencia, seguido de un recorrido por el Cerro San Javier."
                    },
                    {
                        "day": 3,
                        "title": "Valles Calchaquíes: Tafí y Amaicha",
                        "description": "Viaje a través de los valles con paradas en Tafí del Valle y el Museo de la Pachamama en Amaicha."
                    },
                    {
                        "day": 4,
                        "title": "Ruinas de Quilmes y Vinos en Cafayate",
                        "description": "Exploración de las Ruinas de Quilmes y visita a una bodega en Cafayate con almuerzo incluido."
                    },
                    {
                        "day": 5,
                        "title": "Quebrada de las Conchas y Salta",
                        "description": "Recorrido por las formaciones rocosas de la Quebrada de las Conchas y llegada a la ciudad de Salta."
                    },
                    {
                        "day": 6,
                        "title": "Hacia Jujuy: Purmamarca y Tilcara",
                        "description": "Visita al Cerro de los 7 Colores en Purmamarca y el Pucará de Tilcara rumbo al norte."
                    },
                    {
                        "day": 7,
                        "title": "Serranía del Hornocal y Humahuaca",
                        "description": "Excursión al mirador del Hornocal (14 colores) y recorrido por el pueblo histórico de Humahuaca."
                    },
                    {
                        "day": 8,
                        "title": "Salinas Grandes y Regreso",
                        "description": "Ascenso por la Cuesta de Lipán hasta las Salinas Grandes antes del regreso a Salta."
                    }
                ]
            }
